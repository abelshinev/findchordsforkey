{
  "expo": {
    "name": "FindMyChords",
    "slug": "chordsotkey",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/guitar-icon.png",
    "scheme": "chordsotkey",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-guitar.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.abelshinev.FindMyChords"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-guitar.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c78bb0f5-4631-4ad8-be3e-6fb0994eb055"
      }
    },
    "owner": "abelshinev"
  }
}
